                textboxinfo.TextColor3 = Color3.fromRGB(255, 255, 255)
                textboxinfo.TextSize = 16.000
                textboxinfo.TextXAlignment = Enum.TextXAlignment.Left
    
                textboxcontainer.Name = "textboxcontainer"
                textboxcontainer.Parent = textboxcomponent
                textboxcontainer.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                textboxcontainer.Position = UDim2.new(0.600000024, 0, 0.175999999, 0)
                textboxcontainer.Size = UDim2.new(0, 134, 0, 22)
    
