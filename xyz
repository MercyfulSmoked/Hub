                check_box.Parent = checkNo
                check_box.BackgroundTransparency = 1.000
                check_box.LayoutOrder = 4
                check_box.Size = UDim2.new(1, 0, 1, 0)
                check_box.ZIndex = 2
                check_box.Image = "rbxassetid://3926311105"
                check_box.ImageColor3 = Color3.fromRGB(22, 168, 76)
                check_box.ImageRectOffset = Vector2.new(4, 836)
                check_box.ImageRectSize = Vector2.new(48, 48)
                check_box.ImageTransparency = 1.000
        
                ToggleButton.MouseEnter:Connect(function()
                    game.TweenService:Create(ToggleButton, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    }):Play()
                end)
                ToggleButton.MouseLeave:Connect(function()
                    game.TweenService:Create(ToggleButton, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    }):Play()
                end)
                if current then
                    pcall(callback, tog)
                    check_box.ImageTransparency = 0
                    checkNo.ImageColor3 = Color3.fromRGB(22, 168, 76)
                end
            end

            function components:CreateLabel(title)
                title = title or "Label"
                local functions = {}
    
                local TextLabel = Instance.new("TextLabel")
                local UICorner = Instance.new("UICorner")
    
                TextLabel.Parent = sectionsFrame
                TextLabel.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                TextLabel.BorderSizePixel = 0
                TextLabel.Size = UDim2.new(0, 381, 0, 34)
                TextLabel.Font = Enum.Font.Gotham
                TextLabel.Text = "  "..title
                TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextLabel.TextSize = 16.000
                TextLabel.TextXAlignment = Enum.TextXAlignment.Left
    
                UICorner.CornerRadius = UDim.new(0, 4)
                UICorner.Parent = TextLabel
    
                function functions:Update(newTitle)
                    if TextLabel.Text ~= newTitle then
                        TextLabel.Text = "  "..newTitle 
                    end
                end
                return functions
            end

            function components:CreateSlider(title, default, minvalue, maxvalue, callback)
                title = title or "Slider"
                minvalue = minvalue or 1
                maxvalue = maxvalue or 100
                callback = callback or function() end
                local disabled = false
                local functions = {}

                local slidercomponent = Instance.new("Frame")
                local UICorner = Instance.new("UICorner")
                local sliderTitle = Instance.new("TextLabel")
                local SliderButton = Instance.new("TextButton")
                local UICorner_2 = Instance.new("UICorner")
                local UIListLayout = Instance.new("UIListLayout")
                local DragSlider = Instance.new("Frame")
                local UICorner_3 = Instance.new("UICorner")
                local Circle = Instance.new("Frame")
                local UICorner_4 = Instance.new("UICorner")
                local UIListLayout_2 = Instance.new("UIListLayout")
                local UIPadding = Instance.new("UIPadding")
                local sliderNumber = Instance.new("TextLabel")
    
                slidercomponent.Name = "slider-component"
                slidercomponent.Parent = sectionsFrame
                slidercomponent.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                slidercomponent.Position = UDim2.new(0.0517647043, 0, 0.474734008, 0)
                slidercomponent.Size = UDim2.new(0, 381, 0, 45)                
    
                UICorner.CornerRadius = UDim.new(0, 4)
                UICorner.Parent = slidercomponent
    
                sliderTitle.Name = "sliderTitle"
                sliderTitle.Parent = slidercomponent
                sliderTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                sliderTitle.BackgroundTransparency = 1.000
                sliderTitle.Position = UDim2.new(0, 0, 0.0588236488, 0)
                sliderTitle.Size = UDim2.new(0, 290, 0, 26)
                sliderTitle.Font = Enum.Font.Gotham
                sliderTitle.Text = "  "..title
                sliderTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
                sliderTitle.TextSize = 16.000
                sliderTitle.TextXAlignment = Enum.TextXAlignment.Left

                local Value;
                local mouse = game.Players.LocalPlayer:GetMouse()
                local uis = game:GetService("UserInputService")

                SliderButton.Name = "SliderButton"
                SliderButton.Parent = slidercomponent
                SliderButton.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                SliderButton.Position = UDim2.new(0.0799999982, 0, 0.644999981, 0)
                SliderButton.Size = UDim2.new(0, 320, 0, 7)
                SliderButton.AutoButtonColor = false
                SliderButton.Font = Enum.Font.SourceSans
                SliderButton.Text = ""
                SliderButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                SliderButton.TextSize = 14.000
                SliderButton.MouseButton1Down:Connect(function()
                    if not disabled then
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 320) * DragSlider.AbsoluteSize.X) + tonumber(minvalue)) or 0
                        pcall(function()
                            callback(Value)
                            sliderNumber.Text = Value
                        end)
                        DragSlider.Size = UDim2.new(0, math.clamp(mouse.X - DragSlider.AbsolutePosition.X, 0, 318), 0, 7)
                        moveconnection = mouse.Move:Connect(function()
                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 320) * DragSlider.AbsoluteSize.X) + tonumber(minvalue))
                            pcall(function()
                                callback(Value)
                                sliderNumber.Text = Value
                            end)
                            DragSlider.Size = UDim2.new(0, math.clamp(mouse.X - DragSlider.AbsolutePosition.X, 0, 320), 0, 7)
                        end)
                        releaseconnection = uis.InputEnded:Connect(function(Mouse)
                            if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 320) * DragSlider.AbsoluteSize.X) + tonumber(minvalue))
                                pcall(function()
                                    callback(Value)
                                    sliderNumber.Text = Value
                                end)
                                DragSlider.Size = UDim2.new(0, math.clamp(mouse.X - DragSlider.AbsolutePosition.X, 0, 320), 0, 7)
                                moveconnection:Disconnect()
                                releaseconnection:Disconnect()
                            end
                        end)
                    end
                end)
    
                UICorner_2.Parent = SliderButton
                local perc = default/maxvalue
                UIListLayout.Parent = SliderButton
                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
    
                DragSlider.Name = "Drag-Slider"
                DragSlider.Parent = SliderButton
                DragSlider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                wait()
                DragSlider.Size = UDim2.new(perc, 0, 1, 0)
    
                UICorner_3.Parent = DragSlider
    
                Circle.Name = "Circle"
                Circle.Parent = DragSlider
                Circle.BackgroundColor3 = Color3.fromRGB(86, 86, 86)
                Circle.Size = UDim2.new(0, 12, 0, 12)
    
                UICorner_4.Parent = Circle
    
                UIListLayout_2.Parent = DragSlider
                UIListLayout_2.HorizontalAlignment = Enum.HorizontalAlignment.Right
                UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout_2.VerticalAlignment = Enum.VerticalAlignment.Center
    
                UIPadding.Parent = DragSlider
                UIPadding.PaddingRight = UDim.new(0, -5)
    
                sliderNumber.Name = "Slider-Title"
                sliderNumber.Parent = slidercomponent
                sliderNumber.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                sliderNumber.BackgroundTransparency = 1.000
                sliderNumber.Position = UDim2.new(0.790000021, 0, 0.0370000005, 0)
                sliderNumber.Size = UDim2.new(0, 64, 0, 23)
                sliderNumber.Font = Enum.Font.Gotham
                sliderNumber.Text = default
                sliderNumber.TextColor3 = Color3.fromRGB(255, 255, 255)
                sliderNumber.TextSize = 16.000
                sliderNumber.TextXAlignment = Enum.TextXAlignment.Right
        
                slidercomponent.MouseEnter:Connect(function()
                    if not disabled then
                        game.TweenService:Create(slidercomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                        }):Play()
                    end
                end)
                slidercomponent.MouseLeave:Connect(function()
                    if not disabled then
                        game.TweenService:Create(slidercomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                        }):Play()
                    end
                end)
    
                function functions:Update(off, newtitle)
                    newtitle = newtitle or nil
                    if newtitle ~= nil then
                        sliderTitle.Text = newtitle
                    end
    
                    if off then
                        game.TweenService:Create(sliderTitle, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            TextColor3 = Color3.fromRGB(121, 121, 121)
                        }):Play()
                        game.TweenService:Create(sliderNumber, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            TextColor3 = Color3.fromRGB(77, 77, 77)
                        }):Play()
                        game.TweenService:Create(DragSlider, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            BackgroundColor3 = Color3.fromRGB(77, 77, 77)
                        }):Play()
                        disabled = true
                    else
                        game.TweenService:Create(sliderTitle, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            TextColor3 = Color3.fromRGB(255,255,255)
                        }):Play()
                        game.TweenService:Create(sliderNumber, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            TextColor3 = Color3.fromRGB(255,255,255)
                        }):Play()
                        game.TweenService:Create(DragSlider, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            BackgroundColor3 = Color3.fromRGB(255,255,255)
                        }):Play()
                        disabled = false
                    end
                end
                return functions
            end
            function components:CreateTextbox(title, callback)
                title = title or "nil"
                callback = callback or function() end
    
                local textboxcomponent = Instance.new("Frame")
                local UICorner = Instance.new("UICorner")
                local textboxinfo = Instance.new("TextLabel")
                local textboxcontainer = Instance.new("Frame")
                local UICorner_2 = Instance.new("UICorner")
                local UIListLayout = Instance.new("UIListLayout")
                local toolPencil = Instance.new("ImageButton")
                local TextBox = Instance.new("TextBox")
    
