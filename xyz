                TextBox.Parent = textboxcontainer
                TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                TextBox.BackgroundTransparency = 1.000
                TextBox.ClipsDescendants = true
                TextBox.Position = UDim2.new(0.0373138897, 0, 0.0454545468, 0)
                TextBox.Size = UDim2.new(0, 108, 0, 20)
                TextBox.ClearTextOnFocus = false
                TextBox.Font = Enum.Font.Gotham
                TextBox.Text = ""
                TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
                TextBox.TextSize = 10.000
                TextBox.FocusLost:Connect(function(EnterPressed)
                    if not EnterPressed then 
                        return
                    else
                        callback(TextBox.Text)
                        wait(0.10)
                        TextBox.Text = ""  
                        game.TweenService:Create(toolPencil, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                            ImageColor3 = Color3.fromRGB(240, 240, 240)
                        }):Play()
                    end
                end)
    
                TextBox.Focused:Connect(function()
                    game.TweenService:Create(toolPencil, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                        ImageColor3 = Color3.fromRGB(255, 255, 255)
                    }):Play()
                end)

                textboxcomponent.MouseEnter:Connect(function()
                    game.TweenService:Create(textboxcomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    }):Play()
                end)
                textboxcomponent.MouseLeave:Connect(function()
                    game.TweenService:Create(textboxcomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    }):Play()
                end)
            end

            function components:CreateDropdown(title, list, callback)
                title = title or "Dropdown"
                list = list or {}
                callback = callback or function() end
                local functions = {}
                local dropdowncomponent = Instance.new("Frame")
                local DropOpen = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")
                local keyboard_arrow_down = Instance.new("ImageButton")
                local dropcover = Instance.new("Frame")
                local UICorner_2 = Instance.new("UICorner")
                local UIListLayout = Instance.new("UIListLayout")
                local btn = DropOpen
                local dropped = false
    
                dropdowncomponent.Name = "dropdown-component"
                dropdowncomponent.Parent = sectionsFrame
                dropdowncomponent.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                dropdowncomponent.ClipsDescendants = true
                dropdowncomponent.Position = UDim2.new(0.0207261294, 0, 0.0335825644, 0)
                dropdowncomponent.Size = UDim2.new(0, 381, 0, 34)
    
                DropOpen.Name = "DropOpen"
                DropOpen.Parent = dropdowncomponent
                DropOpen.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                DropOpen.Size = UDim2.new(1, 0, 0, 34)
                DropOpen.AutoButtonColor = false
                DropOpen.Font = Enum.Font.Gotham
                DropOpen.Text = "  "..title
                DropOpen.ClipsDescendants = true
                DropOpen.TextColor3 = Color3.fromRGB(255, 255, 255)
                DropOpen.TextSize = 16.000
                DropOpen.TextXAlignment = Enum.TextXAlignment.Left
                DropOpen.MouseButton1Click:Connect(function()
                    if not dropped then
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = true
                            end
                        end
                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, UIListLayout.AbsoluteContentSize.Y + 5)
                        }):Play()
                        game.TweenService:Create(keyboard_arrow_down, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Rotation = 180
                        }):Play()
                        game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundTransparency = 0
                        }):Play()
                        wait(0.1)
                        --UpdateSize()
                        dropped = true
                    else
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = false
                            end
                        end
                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, 34)
                        }):Play()
                        game.TweenService:Create(keyboard_arrow_down, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Rotation = 0
                        }):Play()
                        game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundTransparency = 1
                        }):Play()
                        wait(0.1)
                        --UpdateSize()
                        --UIListLayout.AbsoluteContentSize.Y
                        dropped = false
                    end
                    DropOpen.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    wait(0.01)
                    DropOpen.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                end)
    
                DropOpen.MouseEnter:Connect(function()
                    game.TweenService:Create(DropOpen, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    }):Play()
                    game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    }):Play()
                end)
                DropOpen.MouseLeave:Connect(function()
                    game.TweenService:Create(DropOpen, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    }):Play()
                    game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    }):Play()
                end)
    
                UICorner.CornerRadius = UDim.new(0, 4)
                UICorner.Parent = DropOpen
    
                keyboard_arrow_down.Name = "keyboard_arrow_down"
                keyboard_arrow_down.Parent = DropOpen
                keyboard_arrow_down.AnchorPoint = Vector2.new(0, 1)
                keyboard_arrow_down.BackgroundTransparency = 1.000
                keyboard_arrow_down.LayoutOrder = 15
                keyboard_arrow_down.Position = UDim2.new(0.916000009, 0, 0.79400003, 0)
                keyboard_arrow_down.Size = UDim2.new(0, 20, 0, 20)
                keyboard_arrow_down.ZIndex = 2
                keyboard_arrow_down.Image = "rbxassetid://3926305904"
                keyboard_arrow_down.ImageRectOffset = Vector2.new(404, 284)
                keyboard_arrow_down.ImageRectSize = Vector2.new(36, 36)
    
                dropcover.Name = "drop-cover"
                dropcover.Parent = DropOpen
                dropcover.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
                dropcover.BackgroundTransparency = 1.000
                dropcover.BorderSizePixel = 0
                dropcover.Position = UDim2.new(0, 0, 0.794000089, 0)
                dropcover.Size = UDim2.new(0, 381, 0, 7)--381
    
                UICorner_2.CornerRadius = UDim.new(0, 4)
                UICorner_2.Parent = dropdowncomponent
    
                UIListLayout.Parent = dropdowncomponent
                UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout.Padding = UDim.new(0, 5)
    
                dropdowncomponent.ChildAdded:Connect(function()
                    if dropped then
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = false
                            end
                        end
                        wait(0)
                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, UIListLayout.AbsoluteContentSize.Y + 5)
                        }):Play()
                        wait(0.1)
                        --UpdateSize()
                    end
                end)
                dropdowncomponent.ChildRemoved:Connect(function()
                    if dropped then
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = false
                            end
                        end
                        wait()
                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, UIListLayout.AbsoluteContentSize.Y + 5)
                        }):Play()
                        wait(0.1)
                        --UpdateSize()
                    end
                end)
                --UpdateSize()
                for i,v in next, list do
                    local object = Instance.new("TextButton")
                    local UICorner_3 = Instance.new("UICorner")
                    local btn = object
                    --local Sample = Instance.new("ImageLabel")
    
                    object.Name = "object"
                    object.Parent = dropdowncomponent
                    object.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    object.Position = UDim2.new(0.125984251, 0, 0.348214298, 0)
                    object.Size = UDim2.new(0, 361, 0, 29)
                    object.AutoButtonColor = false
                    object.Font = Enum.Font.Gotham
                    object.ClipsDescendants = true
                    object.Text = v
                    object.TextColor3 = Color3.fromRGB(255, 255, 255)
                    object.TextSize = 16.000
                    object.MouseButton1Click:Connect(function()
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = false
                            end
                        end
                        callback(v)
                        DropOpen.Text = "  "..title.." - "..v
                        dropdowncomponent:TweenSize(UDim2.new(0, 381, 0, 34), "In", "Quad", 0.1)
                        game.TweenService:Create(keyboard_arrow_down, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Rotation = 0
                        }):Play()
                        game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundTransparency = 1
                        }):Play()
                        dropped = false
                        wait(0.1)
                        --UpdateSize()
                    end)
                    --while wait() do
                    --    for i,v in pairs(dropdowncomponent:GetChildren()) do
                    --        if v.Name == "object" then
                    --            v.Visible = false
                    --        end
                    --    end
                    --end
                    UICorner_3.CornerRadius = UDim.new(0, 4)
                    UICorner_3.Parent = object
    
                    object.MouseEnter:Connect(function()
                        game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                        }):Play()
                    end)
                    object.MouseLeave:Connect(function()
                        game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                        }):Play()
                    end)
                end
                if not dropped then
                    for i,v in pairs(dropdowncomponent:GetChildren()) do
                        if v.Name == "object" then
                            v.Visible = false
                        end
                    end
                else
                    for i,v in pairs(dropdowncomponent:GetChildren()) do
                        if v.Name == "object" then
                            v.Visible = true
                        end
                    end
                end
                function functions:Refresh(newList)
                    for i,v in pairs(dropdowncomponent:GetChildren()) do
                        if v.Name == "object" then
                            v:Destroy()
                        end
                    end
    
                    for i,v in next, newList do
                        local object = Instance.new("TextButton")
                        local UICorner_3 = Instance.new("UICorner")
                        local btn = object
                        local Sample = Instance.new("ImageLabel")
        
                        object.Name = "object"
                        object.Parent = dropdowncomponent
                        object.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                        object.Position = UDim2.new(0.125984251, 0, 0.348214298, 0)
                        object.Size = UDim2.new(0, 361, 0, 29)
                        object.AutoButtonColor = false
                        object.Font = Enum.Font.Gotham
                        object.ClipsDescendants = true
                        object.Text = v
                        object.TextColor3 = Color3.fromRGB(255, 255, 255)
                        object.TextSize = 16.000
                        object.MouseButton1Click:Connect(function()
                            for i,v in pairs(dropdowncomponent:GetChildren()) do
                                if v.Name == "object" then
                                    v.Visible = false
                                end
                            end
                            callback(v)
                            DropOpen.Text = "  "..title.." - "..v
                            dropdowncomponent:TweenSize(UDim2.new(0, 381, 0, 34), "In", "Quad", 0.1)
                            game.TweenService:Create(keyboard_arrow_down, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                Rotation = 0
                            }):Play()
                            game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                BackgroundTransparency = 1
                            }):Play()
                            dropped = false
                            wait(0.1)
                            --UpdateSize()
                        end)
            
                        UICorner_3.CornerRadius = UDim.new(0, 4)
                        UICorner_3.Parent = object
    
                        object.MouseEnter:Connect(function()
                            game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                            }):Play()
                        end)
                        object.MouseLeave:Connect(function()
                            game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                            }):Play()
                        end)
                    end
                end
                return functions
            end

            function components:CreateKeybind(title, currentKey, callback)
                local first = currentKey.Name
    
                local keycomponent = Instance.new("Frame")
                local keybtn = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")
                local Frame = Instance.new("Frame")
                local UIListLayout = Instance.new("UIListLayout")
                local kkey = Instance.new("TextLabel")
                local UICorner_2 = Instance.new("UICorner")
    
                keycomponent.Name = "key-component"
                keycomponent.Parent = sectionsFrame
                keycomponent.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                keycomponent.BackgroundTransparency = 1.000
                keycomponent.Position = UDim2.new(0.0207261294, 0, 0.0168062188, 0)
                keycomponent.Size = UDim2.new(0, 381, 0, 34)
    
                keybtn.Name = "keybtn"
                keybtn.Parent = keycomponent
                keybtn.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                keybtn.Size = UDim2.new(1, 0, 1, 0)
                keybtn.AutoButtonColor = false
                keybtn.Font = Enum.Font.Gotham
                keybtn.Text = "  "..title
                keybtn.TextColor3 = Color3.fromRGB(255, 255, 255)
                keybtn.TextSize = 16.000
                keybtn.TextXAlignment = Enum.TextXAlignment.Left
                keybtn.MouseButton1Click:Connect(function()
                    kkey.Text = ". . ."
                    kkey.Size = UDim2.new(0, math.max(46, kkey.TextBounds.X + 24), 0, math.max(24, kkey.TextBounds.Y))
                    local a, b = game:GetService('UserInputService').InputBegan:wait();
                    if a.KeyCode.Name ~= "Unknown" then
                        kkey.Text = a.KeyCode.Name
                        first = a.KeyCode.Name;
                        kkey.Size = UDim2.new(0, math.max(46, kkey.TextBounds.X + 24), 0, math.max(24, kkey.TextBounds.Y))
                    end	
                end)
    
                game:GetService("UserInputService").InputBegan:connect(function(current, ok) 
                    if not ok then 
                        if current.KeyCode.Name == first then 
                            callback()
                        end
                    end
                end)
    
                UICorner.CornerRadius = UDim.new(0, 4)
                UICorner.Parent = keybtn
    
                Frame.Parent = keybtn
                Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Frame.BackgroundTransparency = 1.000
                Frame.Position = UDim2.new(0.588, 0, 0, 0)
                Frame.Size = UDim2.new(0, 149, 0, 34)                
    
                UIListLayout.Parent = Frame
                UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
    
                kkey.Name = "kkey"
                kkey.Parent = Frame
                kkey.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                kkey.Position = UDim2.new(0.691275179, 0, 0.14705883, 0)
                kkey.Size = UDim2.new(0, 46, 0, 24)
                kkey.Font = Enum.Font.Gotham
                kkey.Text = first
                kkey.TextColor3 = Color3.fromRGB(255, 255, 255)
                kkey.TextSize = 14.000
    
                UICorner_2.CornerRadius = UDim.new(0, 4)
                UICorner_2.Parent = kkey           
                kkey.Size = UDim2.new(0, math.max(46, kkey.TextBounds.X + 24), 0, math.max(24, kkey.TextBounds.Y))
            end
            function components:CreateChoice(title, list, callback)
                title = title or "Multi-Dropdown"
                list = list or {}
                callback = callback or function() end
                local functions = {}
                local selectedList = {}
                local dropdowncomponent = Instance.new("Frame")
                local DropOpen = Instance.new("TextButton")
                local UICorner = Instance.new("UICorner")
                local keyboard_arrow_down = Instance.new("ImageButton")
                local dropcover = Instance.new("Frame")
                local UICorner_2 = Instance.new("UICorner")
                local UIListLayout = Instance.new("UIListLayout")
                local btn = DropOpen
                local dropped = false

                dropdowncomponent.Name = "dropdown-component"
                dropdowncomponent.Parent = sectionsFrame
                dropdowncomponent.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                dropdowncomponent.ClipsDescendants = true
                dropdowncomponent.Position = UDim2.new(0.0207261294, 0, 0.0335825644, 0)
                dropdowncomponent.Size = UDim2.new(0, 381, 0, 34)
    
                DropOpen.Name = "DropOpen"
                DropOpen.Parent = dropdowncomponent
                DropOpen.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                DropOpen.Size = UDim2.new(1, 0, 0, 34)
                DropOpen.AutoButtonColor = false
                DropOpen.Font = Enum.Font.Gotham
                DropOpen.Text = "  "..title
                DropOpen.ClipsDescendants = true
                DropOpen.TextColor3 = Color3.fromRGB(255, 255, 255)
                DropOpen.TextSize = 16.000
                DropOpen.TextXAlignment = Enum.TextXAlignment.Left
                DropOpen.MouseButton1Click:Connect(function()
                    if not dropped then
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = true
                            end
                        end

                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, UIListLayout.AbsoluteContentSize.Y + 5)
                        }):Play()
                        game.TweenService:Create(keyboard_arrow_down, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Rotation = 180
                        }):Play()
                        game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundTransparency = 0
                        }):Play()
                        wait(0.1)

                        dropped = true
                    else
                        for i,v in pairs(dropdowncomponent:GetChildren()) do
                            if v.Name == "object" then
                                v.Visible = false
                            end
                        end

                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, 34)
                        }):Play()
                        game.TweenService:Create(keyboard_arrow_down, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Rotation = 0
                        }):Play()
                        game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            BackgroundTransparency = 1
                        }):Play()
                        wait(0.1)

                        dropped = false
                    end
                    DropOpen.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    wait(0.01)
                    DropOpen.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                end)
    
                DropOpen.MouseEnter:Connect(function()
                    game.TweenService:Create(DropOpen, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    }):Play()
                    game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                    }):Play()
                end)
                DropOpen.MouseLeave:Connect(function()
                    game.TweenService:Create(DropOpen, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    }):Play()
                    game.TweenService:Create(dropcover, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                        BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    }):Play()
                end)
    
                UICorner.CornerRadius = UDim.new(0, 4)
                UICorner.Parent = DropOpen
    
                keyboard_arrow_down.Name = "keyboard_arrow_down"
                keyboard_arrow_down.Parent = DropOpen
                keyboard_arrow_down.AnchorPoint = Vector2.new(0, 1)
                keyboard_arrow_down.BackgroundTransparency = 1.000
                keyboard_arrow_down.LayoutOrder = 15
                keyboard_arrow_down.Position = UDim2.new(0.916000009, 0, 0.79400003, 0)
                keyboard_arrow_down.Size = UDim2.new(0, 20, 0, 20)
                keyboard_arrow_down.ZIndex = 2
                keyboard_arrow_down.Image = "rbxassetid://3926305904"
                keyboard_arrow_down.ImageRectOffset = Vector2.new(404, 284)
                keyboard_arrow_down.ImageRectSize = Vector2.new(36, 36)            
    
                dropcover.Name = "drop-cover"
                dropcover.Parent = DropOpen
                dropcover.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
                dropcover.BackgroundTransparency = 1.000
                dropcover.BorderSizePixel = 0
                dropcover.Position = UDim2.new(0, 0, 0.794000089, 0)
                dropcover.Size = UDim2.new(0, 381, 0, 7)--381
    
                UICorner_2.CornerRadius = UDim.new(0, 4)
                UICorner_2.Parent = dropdowncomponent
    
                UIListLayout.Parent = dropdowncomponent
                UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout.Padding = UDim.new(0, 5)
    
                dropdowncomponent.ChildAdded:Connect(function()
                    if dropped then
                        wait(0)
                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, UIListLayout.AbsoluteContentSize.Y + 5)
                        }):Play()
                        wait(0.1)
                    end
                end)
                dropdowncomponent.ChildRemoved:Connect(function()
                    if dropped then
                        wait()
                        game.TweenService:Create(dropdowncomponent, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                            Size = UDim2.new(0,381,0, UIListLayout.AbsoluteContentSize.Y + 5)
                        }):Play()
                        wait(0.1)
                    end
                end)

                for i,v in next, list do
                    local object = Instance.new("TextButton")
                    local UICorner_3 = Instance.new("UICorner")
                    local btn = object
    
                    local isSelected
                    object.Name = "object"
                    object.Parent = dropdowncomponent
                    object.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                    object.Position = UDim2.new(0.125984251, 0, 0.348214298, 0)
                    object.Size = UDim2.new(0, 361, 0, 29)
                    object.AutoButtonColor = false
                    object.Font = Enum.Font.Gotham
                    object.ClipsDescendants = true
                    object.Text = v
                    object.TextColor3 = Color3.fromRGB(255, 255, 255)
                    object.TextSize = 16.000
                    object.MouseButton1Click:Connect(function()
                        if not isSelected then
                            game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                            }):Play()
                            wait(0.1)
                            table.insert(selectedList, object.Text)
                            for i,v in pairs(selectedList) do
                                callback(v)
                            end 
                            isSelected = true
                        else
                            game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                BackgroundColor3 = Color3.fromRGB(43,43,43)
                            }):Play()
                            wait(0.1)
                            for a,b in pairs(selectedList) do
                                if b == v then
                                    table.remove(selectedList, a) 
                                end
                            end
                            wait()
                            for i,v in pairs(selectedList) do
                                callback(v)
                            end 
                            isSelected = false
                        end
                    end)
        
                    UICorner_3.CornerRadius = UDim.new(0, 4)
                    UICorner_3.Parent = object
    
                    --object.MouseEnter:Connect(function()
                        --if not isSelected then
                            --game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                --BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                            --}):Play()
                        --end
                    --end)
                    --object.MouseLeave:Connect(function()
                        --if not isSelected then
                            --game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                --BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                            --}):Play()
                        --end
                    --end)
                end

                if not dropped then
                    for i,v in pairs(dropdowncomponent:GetChildren()) do
                        if v.Name == "object" then
                            v.Visible = false
                        end
                    end
                else
                    for i,v in pairs(dropdowncomponent:GetChildren()) do
                        if v.Name == "object" then
                            v.Visible = true
                        end
                    end
                end

                function functions:Refresh(newList)
                    for i,v in pairs(dropdowncomponent:GetChildren()) do
                        if v.Name == "object" then
                            v:Destroy()
                        end
                    end
    
                    for i,v in next, newList do
                        local object = Instance.new("TextButton")
                        local UICorner_3 = Instance.new("UICorner")
                        local btn = object
                        local Sample = Instance.new("ImageLabel")
        
                        object.Name = "object"
                        object.Parent = dropdowncomponent
                        object.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                        object.Position = UDim2.new(0.125984251, 0, 0.348214298, 0)
                        object.Size = UDim2.new(0, 361, 0, 29)
                        object.AutoButtonColor = false
                        object.Font = Enum.Font.Gotham
                        object.ClipsDescendants = true
                        object.Text = v
                        object.TextColor3 = Color3.fromRGB(255, 255, 255)
                        object.TextSize = 16.000
                        object.MouseButton1Click:Connect(function()
                            if not isSelected then
                                game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                    BackgroundColor3 = Color3.fromRGB(50,50,50)
                                }):Play()
                                wait(0.1)
                                table.insert(selectedList, object.Text)
                                for i,v in pairs(selectedList) do
                                    callback(v)
                                end 
                                isSelected = true
                            else
                                game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                    BackgroundColor3 = Color3.fromRGB(43,43,43)
                                }):Play()
                                wait(0.1)
                                for a,b in pairs(selectedList) do
                                    if b == v then
                                        table.remove(selectedList, a) 
                                    end
                                end
                                wait()
                                for i,v in pairs(selectedList) do
                                    callback(v)
                                end 
                                isSelected = false
                            end
                        end)
            
                        UICorner_3.CornerRadius = UDim.new(0, 4)
                        UICorner_3.Parent = object
    
                        object.MouseEnter:Connect(function()
                            if not isSelected then
                                game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                    BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                                }):Play()
                            end
                        end)
                        object.MouseLeave:Connect(function()
                            if not isSelected then
                                game.TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {
                                    BackgroundColor3 = Color3.fromRGB(43, 43, 43)
                                }):Play()
                            end
                        end)
                    end
                end
                return functions
            end

            return components
        end
        return sComponents
    end
    return tComponents
end

return Library
